include_rules

srcs += $(MOZ_ROOT)/rdf/util/src/*.cpp
srcs += $(MOZ_ROOT)/intl/unicharutil/util/*.cpp
# nsUnicodePropertyData.cpp is included, so put the files in the
# <installed-headers> group.
: foreach $(srcs) |> !ln |> %b | $(MOZ_ROOT)/<installed-headers>

include $(MOZ_ROOT)/build/tup/compile.tup
