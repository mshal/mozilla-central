include_rules

histograms_file = Histograms.json

: |> $(PYTHON) gen-histogram-enum.py $(histograms_file) > %o |> TelemetryHistogramEnums.h | $(MOZ_ROOT)/<generated-headers>

: |> $(PYTHON) gen-histogram-data.py $(histograms_file) > %o |> TelemetryHistogramData.inc | $(MOZ_ROOT)/<generated-headers>

export SHELL

include $(MOZ_ROOT)/build/tup/compile.tup
