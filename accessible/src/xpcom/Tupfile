include_rules

: | $(MOZ_ROOT)/<installed-idls> |> ^ python AccEventGen.py -> %o^ $(PYTHON_PATH) -I$(MOZ_ROOT)/other-licenses/ply -I$(MOZ_ROOT)/python/codegen -I$(MOZ_ROOT)/xpcom/idl-parser AccEventGen.py -I$(DIST)/idl --header-output %o AccEvents.conf |> xpcAccEvents.h | $(MOZ_ROOT)/<generated-headers>
: | $(MOZ_ROOT)/<installed-idls> |> ^ python AccEventGen.py -> %o^ $(PYTHON_PATH) -I$(MOZ_ROOT)/other-licenses/ply -I$(MOZ_ROOT)/python/codegen -I$(MOZ_ROOT)/xpcom/idl-parser AccEventGen.py -I$(DIST)/idl --stub-output %o AccEvents.conf |> xpcAccEvents.cpp

include $(MOZ_ROOT)/build/tup/compile.tup
