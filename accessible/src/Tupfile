include_rules

ifeq ($(MOZ_WIDGET_TOOLKIT),gtk2)
PLATFORM_DIR = atk
else
ifeq ($(MOZ_WIDGET_TOOLKIT),windows)
PLATFORM_DIR = msaa
else
ifeq ($(MOZ_WIDGET_TOOLKIT),cocoa)
PLATFORM_DIR = mac
else
PLATFORM_DIR = other
endif
endif
endif

: $(PLATFORM_DIR)/libaccessibility_toolkit_s.a.desc |> !cp |>
