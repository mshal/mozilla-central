include_rules

# List of directories that contain EXPORTS_mozilla
EXPORT_DIRS += $(MOZ_ROOT)/dom/bindings
EXPORT_DIRS += $(MOZ_ROOT)/dom/browser-element
EXPORT_DIRS += $(MOZ_ROOT)/dom/ipc
EXPORT_DIRS += $(MOZ_ROOT)/dom/media
EXPORT_DIRS += $(MOZ_ROOT)/dom/plugins/base
EXPORT_DIRS += $(MOZ_ROOT)/dom/plugins/ipc
EXPORT_DIRS += $(MOZ_ROOT)/dom/system

run python -B $(MOZ_ROOT)/build/pymake/tup_export.py EXPORTS_mozilla $(MOZ_ROOT) $(EXPORT_DIRS)
