include_rules

# List of directories that contain EXPORTS_mtransport

# Both of these directories define EXPORTS_mtransport, but the ones in
# standalone appear to be a subset of the ones in build.
EXPORT_DIRS += $(MOZ_ROOT)/media/mtransport/build
#EXPORT_DIRS += $(MOZ_ROOT)/media/mtransport/standalone

run $(PYTHON) $(MOZ_ROOT)/build/pymake/tup_export.py EXPORTS_mtransport $(MOZ_ROOT) @(MOZ_OBJDIR) $(EXPORT_DIRS)
