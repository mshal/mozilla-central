include_rules

export SHELL

# List of directories that contain EXPORTS
EXPORT_DIRS += $(MOZ_ROOT)/security/dbm/include

# --allow-includes needed for mfbt, which defines exports in exported_headers.mk
run $(PYTHON) $(MOZ_ROOT)/build/pymake/tup_export.py --allow-includes -m Makefile EXPORTS $(MOZ_ROOT) @(MOZ_OBJDIR) $(EXPORT_DIRS)
