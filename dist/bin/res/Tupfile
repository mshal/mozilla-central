include_rules

res_dirs += $(MOZ_ROOT)/editor/composer/src
res_dirs += $(MOZ_ROOT)/layout/style

run $(PYTHON) $(MOZ_ROOT)/build/pymake/tup_export.py _FILES $(MOZ_ROOT) @(MOZ_OBJDIR) $(res_dirs)

run $(PYTHON) $(MOZ_ROOT)/build/pymake/tup_export.py EXPORT_RESOURCE $(MOZ_ROOT) @(MOZ_OBJDIR) $(MOZ_ROOT)/intl/locale/src

run $(PYTHON) $(MOZ_ROOT)/build/pymake/tup_export.py _RES_FILES $(MOZ_ROOT) @(MOZ_OBJDIR) $(MOZ_ROOT)/netwerk/test
