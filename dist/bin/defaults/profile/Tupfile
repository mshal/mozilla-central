include_rules

: $(MOZ_ROOT)/browser/locales/generic/profile/bookmarks.html.in |> ^ Preprocess %f -> %o^ $(PYTHON) $(MOZ_ROOT)/config/Preprocessor.py -I$(MOZ_ROOT)/browser/locales/$(MOZ_UI_LOCALE)/profile/bookmarks.inc -DAB_CD=$(MOZ_UI_LOCALE) %f > %o |> %B

extra_files += $(MOZ_ROOT)/browser/app/profile/prefs.js
extra_files += $(MOZ_ROOT)/browser/locales/generic/profile/localstore.rdf
extra_files += $(MOZ_ROOT)/browser/locales/generic/profile/mimeTypes.rdf
: foreach $(extra_files) |> !ln |>
