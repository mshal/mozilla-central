include_rules

PLATFORM_DIR = unixshared

ifeq ($(MOZ_WIDGET_TOOLKIT),os2)
PLATFORM_DIR = os2
endif

ifeq ($(MOZ_WIDGET_TOOLKIT),windows)
PLATFORM_DIR = win
endif

ifeq ($(MOZ_WIDGET_TOOLKIT),cocoa)
PLATFORM_DIR = mac
endif

: $(PLATFORM_DIR)/libprintingui_s.a.desc |> !cp |>
